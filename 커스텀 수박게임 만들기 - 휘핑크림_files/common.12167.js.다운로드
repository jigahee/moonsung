function cmp(a,b){if(a===b)return!0;if(!a||typeof a!="object"||!b||typeof b!="object")return!1;var props=0;for(var prop in a)if(a.hasOwnProperty(prop)){if(!b.hasOwnProperty(prop)||!cmp(a[prop],b[prop]))return!1;props++}
