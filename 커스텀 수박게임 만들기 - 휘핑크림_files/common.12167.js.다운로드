modeCfg.maxBlockquoteDepth=0;if(modeCfg.taskLists===undefined)modeCfg.taskLists=!1;if(modeCfg.strikethrough===undefined)
modeCfg.strikethrough=!1;if(modeCfg.emoji===undefined)
modeCfg.emoji=!1;if(modeCfg.fencedCodeBlockHighlighting===undefined)
modeCfg.fencedCodeBlockHighlighting=!0;if(modeCfg.fencedCodeBlockDefaultMode===undefined)
