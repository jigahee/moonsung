function toast(text){$.toast({text:text,position:"top-center",bgColor:"#020202",loaderBg:"#3478ff",hideAfter:1500})}
function clearPhotoset(){$(".photoset").each(function(i,val){var $figure=$(val);var len=$figure.find(".photo").length;if(len===0){$figure.remove()}else{var $figureInner=$figure.find(".photoset-inner");$figureInner.removeClass("photo-1");$figureInner.removeClass("photo-2");$figureInner.removeClass("photo-3");$figureInner.addClass("photo-"+len)}})}
function resizePhotoset(){var $figures=$(".photoset");$figures.each(function(i,val){var $figure=$(val);var $figureInner=$figure.find(".photoset-inner");var editorWidth=0;var innerHeight=0;var gutter=4;var imgWidth;var imgHeight;var rate;var $imgs=$figureInner.find(".photo");var $img=null;$figure.find("figcaption").remove();$figure.removeClass("has-figcaption");var captionHtml="";if($imgs.length===1){$img=$imgs;imgWidth=parseInt($img.attr("data-width"));imgHeight=parseInt($img.attr("data-height"));editorWidth=$figure.width();if(editorWidth>imgWidth&&($figure.hasClass("individual")||$figure.hasClass("seamless"))){$img.css({width:imgWidth,height:imgHeight,marginLeft:parseInt((editorWidth-imgWidth)/2)})}else{innerHeight=parseInt(editorWidth*imgHeight/imgWidth);$img.css({width:editorWidth,height:innerHeight})}
if($img.attr("data-caption")===undefined||$img.attr("data-caption")===""){$img.removeClass("has-caption")}else{$img.addClass("has-caption");$figure.addClass("has-figcaption");captionHtml="<figcaption>"+$img.attr("data-caption")+"</figcaption>"}
$figure.append(captionHtml)}else if($imgs.length>1){var captionNumber=0;captionHtml+="<figcaption>";$imgs.each(function(ii,img){$img=$(img);if($img.attr("data-caption")===undefined||$img.attr("data-caption")===""){$img.removeClass("has-caption")}else{$img.addClass("has-caption");captionNumber++;$img.attr("data-caption-number",captionNumber);captionHtml+="<span class='photoset-caption'><span class='photoset-caption-number'>"+captionNumber+"</span>"+$img.attr("data-caption")+"</span>"}});captionHtml+="</figcaption>";if(captionNumber>0){$figure.append(captionHtml)}
var photosetLength=$imgs.length;var column=1;if($figure.hasClass("column-2")){column=2}else if($figure.hasClass("column-3")){column=3}
if($figure.hasClass("individual")){column=1}
if($figure.hasClass("seamless")){column=1;gutter=0}
if(photosetLength<column){column=photosetLength}
if($figure.hasClass("slideshow")){var $inner=$figure.find(".photoset-inner");if($figure.find(".slideshow-helper").length===0){$figure.prepend("<div class='slideshow-helper'><div class='indicator'></div><div class='helper-text'>확대해 보려면 이미지를 누르세요.</div></div>")}
var slideCount=$inner.find("img").length;var $slideShowIndicator=$figure.find(".indicator");$inner.on('init reInit afterChange',function(event,slick,currentSlide,nextSlide){var i=(currentSlide?currentSlide:0)/column;i=parseInt(i)+1;$slideShowIndicator.text(i+'/'+Math.ceil(slideCount/column))});try{$inner.slick({lazyLoad:'anticipated',infinite:!1,nextArrow:"<button type=\"button\" role=\"presentation\" class=\"btn-next\"><i class=\"far fa-chevron-right\"></i></button>",prevArrow:"<button type=\"button\" role=\"presentation\" class=\"btn-prev\"><i class=\"far fa-chevron-left\"></i></button>",slidesToShow:column,slidesToScroll:column,});$inner.on("beforeChange",function(event,slick,currentSlide,nextSlide){})}catch(error){}}else{var m=0;var n;while(!0){var sampleWidth=0;var sampleHeight=10000;var remain=photosetLength%column;var maxLength;if(remain>0&&m<remain){maxLength=remain}else{maxLength=column}
if(maxLength===2){editorWidth=$figure.width()-gutter}else if(maxLength===3){editorWidth=$figure.width()-(gutter*2)}else{editorWidth=$figure.width()}
if(editorWidth===0){m=m+maxLength;break}
if($figure.hasClass("tile-square")){for(n=0;n<maxLength;n++){$img=$($imgs.get(m));$img.css({width:parseInt(editorWidth/maxLength),height:parseInt(editorWidth/maxLength)});if(n!==maxLength-1){$img.css({marginRight:gutter+"px"})}
if(m<photosetLength-column){$img.css({marginBottom:gutter+"px"})}
m++}}else{if(column===1){$img=$($imgs.get(m));$img.removeAttr("style");imgWidth=$img.attr("data-width");if(editorWidth>imgWidth&&($figure.hasClass("individual")||$figure.hasClass("seamless"))){$img.css({width:imgWidth,height:"auto",marginLeft:parseInt((editorWidth-imgWidth)/2)})}else{$img.css({width:editorWidth,height:"auto"})}
if(m<photosetLength-column){$img.css({marginBottom:gutter+"px"})}
m++}else{for(n=0;n<maxLength;n++){$img=$($imgs.get(m+n));imgWidth=parseInt($img.attr("data-width"));imgHeight=parseInt($img.attr("data-height"));sampleWidth+=(imgWidth*sampleHeight)/imgHeight}
rate=sampleWidth/editorWidth;innerHeight=parseInt(sampleHeight/rate);var lastWidth=editorWidth;for(n=0;n<maxLength;n++){$img=$($imgs.get(m));imgWidth=parseInt($img.attr("data-width"));imgHeight=parseInt($img.attr("data-height"));if(n!==maxLength-1){$img.css({marginRight:gutter+"px"})}
if(m<photosetLength-column){$img.css({marginBottom:gutter+"px"})}
var targetWidth=parseInt(imgWidth*innerHeight/imgHeight);if(n===maxLength-1&&maxLength>=2){$img.css({width:lastWidth,height:innerHeight})}else{lastWidth=lastWidth-targetWidth;$img.css({width:targetWidth,height:innerHeight})}
m++}}}
if(m>=photosetLength){break}}}}})}
function resizeIframe(){$(".place iframe").each(function(i,val){var $iframe=$(val);var $editorContainer=$iframe.parents(".element-editor-container");if($editorContainer.length===0){$editorContainer=$iframe.parents("figure")}
var width;var height;if($editorContainer.hasClass("embed")){var $parent=$iframe.parent();var embedWidth=parseInt($parent.attr("data-width"));var embedHeight=parseInt($parent.attr("data-height"));var maxWidth=$parent.parent().width();width=maxWidth;height=parseInt(width*embedHeight/embedWidth);$iframe.width(width).height(height)}else if($editorContainer.hasClass("place")){width=$iframe.parent().width();height=Math.ceil(width/2);$iframe.width(width).height(height)}else{width=$iframe.parent().width();height=$iframe.parent().height();$iframe.width(width).height(height)}})}
function setCodeComponents(){var $codes=$("figure.code, .element-editor-container.code");if($codes.length>0){$codes.each(function(i,val){var $code=$(val);setCodeComponent($code)})}}
function setCodeComponent($code){var $pre=$code.find("pre");var mode=$pre.attr("data-type");var value=$pre.text();CodeMirror($code.get(0),{lineNumbers:!0,lineWrapping:!0,indentUnit:4,inputStyle:"contenteditable",spellcheck:!1,tabSize:4,value:value,mode:mode,readOnly:"nocursor",theme:'monokai'})}
function setAudioComponents(){$(".post-content .audio").each(function(i,val){setAudioComponent($(val))})}
function setAudioComponent($audioPlayList){var audio=audiojs.create($audioPlayList.find("audio").get(0),{createPlayer:{markup:'\
            <div class="audio-component"> \
                <div class="audio-body"> \
                  <div class="audio-cover"> \
                    <span><i class="fas fa-music"></i></span>\
                  </div>\
                  <div class="audio-title-btn">\
                      <div class="audio-playing-title">\
                        <span>title title title title title title title title title</span>\
                      </div>\
                      <div class="audio-btn">\
                          <div class="step-backward"> \
                            <p class="step-btn"><span class="fas fa-backward"></span></p> \
                          </div> \
                          <div class="play-pause"> \
                            <p class="play"><span class="fas fa-play"></span></p> \
                            <p class="pause"><span class="fas fa-pause"></span></p> \
                            <p class="loading"><span class="fas fa-spinner fa-spin"></span></p> \
                            <p class="error"><span class="fas fa-exclamation-triangle"></span></p> \
                          </div> \
                          <div class="step-forward"> \
                            <p class="step-btn"><span class="fas fa-forward"></span></p> \
                          </div> \
                          <div class="audio-playlist"> \
                            <p class="open-playlist active"><span class="fal fa-bars"></span><span class="fal fa-chevron-up"></span></p> \
                          </div> \
                      </div> \
                  </div> \
                </div> \
                <div class="audio-timeline">\
                <div class="time"> \
                    <em class="played">00:00</em>\
                  </div> \
                  <div class="scrubber"> \
                    <div class="progress"></div> \
                    <div class="loaded"></div> \
                  </div> \
                  <div class="time"> \
                    <strong class="duration">00:00</strong> \
                  </div> \
                </div> \
            </div> \
          <div class="error-message"></div>',playPauseClass:'play-pause',scrubberClass:'scrubber',progressClass:'progress',loaderClass:'loaded',timeClass:'time',durationClass:'duration',playedClass:'played',errorMessageClass:'error-message',playingClass:'playing',loadingClass:'loading1',errorClass:'error'},css:!1,trackEnded:function(){let next=$audioPlayList.find('ol li.playing').next();if(!next.length)next=$audioPlayList.find('ol li').first();next.addClass('playing').siblings().removeClass('playing');const $first=$('a',next).first();const first=$first.attr('data-src');$audioPlayList.find(".audio-playing-title").text($first.text());audio.load(first);audio.play()}});var $audioParent=$audioPlayList.closest(".audio");$audioParent.height($audioParent.height());$audioPlayList.find(".audiojs").height($audioPlayList.find(".audiojs").height());var $audioListCover=$audioPlayList.find(".audio-list-cover img");if($audioListCover.length>0){$audioPlayList.find(".audio-cover").html($audioListCover);$audioPlayList.find(".audio-playing-title")}
var $first=$audioPlayList.find('ol a').first();var first=$first.attr('data-src');$audioPlayList.find('ol li').first().addClass('playing');$audioPlayList.find(".audio-playing-title").text($first.text());audio.load(first);$audioPlayList.find('ol li').click(function(e){$(this).addClass('playing').siblings().removeClass('playing');var $first=$('a',this).first();var first=$first.attr('data-src');$audioPlayList.find(".audio-playing-title").text($first.text());audio.load(first);audio.play();return!1});$audioPlayList.find(".step-forward").on("click",function(){var $next=$audioPlayList.find('ol li.playing').next();if(!$next.length)$next=$audioPlayList.find('ol li').first();$next.addClass('playing').siblings().removeClass('playing');var $first=$('a',$next).first();var first=$first.attr('data-src');$audioPlayList.find(".audio-playing-title").text($first.text());audio.load(first);audio.play();return!1});$audioPlayList.find(".step-backward").on("click",function(){var $prev=$audioPlayList.find('ol li.playing').prev();if(!$prev.length)$prev=$audioPlayList.find('ol li').last();$prev.addClass('playing').siblings().removeClass('playing');var $first=$('a',$prev).first();var first=$first.attr('data-src');$audioPlayList.find(".audio-playing-title").text($first.text());audio.load(first);audio.play();return!1});$audioPlayList.find(".open-playlist").on("click",function(){var $btn=$(this);if($btn.hasClass("active")){$audioPlayList.find("ol").hide()}else{$audioPlayList.find("ol").show()}
$btn.toggleClass("active");$audioParent.removeAttr("style");$audioParent.height($audioParent.height());$audioPlayList.find(".audiojs").height($audioPlayList.find(".audiojs").height());return!1})}
function relatedEllipsis(){var relatedArticle=$('.related-post .item-content');$('.related-post').find(relatedArticle).each(function(){var subText=$(this).find('.item-content-text');if(subText.length===0){$(this).find('.text-box').addClass('has-no-subtext')}})}
relatedEllipsis();function closeConfirmPostype(){var $modalConfirm=$("#modal-confirm");$modalConfirm.modal("hide")}
function closeAlertPostype(){var $modalAlert=$("#modal-alert");$modalAlert.modal("hide")}
function confirmPostype(message,positive_callback,negative_callback){var $modalConfirm=$("#modal-confirm");var $modalConfirmYes=$("#modal-confirm-yes");var $modalConfirmNo=$("#modal-confirm-no");var $modalConfirmMessage=$("#modal-confirm-message");$modalConfirm.modal({show:!0});$modalConfirmYes.off("click");$modalConfirmYes.on("click",function(){if(typeof(positive_callback)==="function"){setTimeout(function(){positive_callback()},170)}});$modalConfirmNo.off("click");$modalConfirmNo.on("click",function(){if(typeof(negative_callback)==="function"){setTimeout(function(){negative_callback()},170)}});$modalConfirmMessage.html(message)}
function alertPostype(message,confirm_callback){var $modalAlert=$("#modal-alert");var $modalAlertConfirm=$("#modal-alert-confirm");var $modalAlertMessage=$("#modal-alert-message");$modalAlert.modal({show:!0});$modalAlertConfirm.off("click");$modalAlertConfirm.on("click",function(){if(typeof(confirm_callback)==="function"){setTimeout(function(){confirm_callback()},170)}});setTimeout(function(){$modalAlertConfirm.focus()},500);$modalAlertMessage.html(message)}
function showLoading(){$(".loading").show()}
function hideLoading(){$(".loading").hide()}
function preventBackConfirm(){$(window).off('beforeunload')}
function diffTime(time){var diff=(((new Date()).getTime()-(time*1000))/1000);var day_diff;if(diff<0)diff=0;day_diff=Math.floor(diff/86400);if(isNaN(day_diff)||day_diff<0)
return;return day_diff===0&&(diff<60&&"방금 전"||diff<120&&"1분 전"||diff<3600&&Math.floor(diff/60)+"분 전"||diff<7200&&"1시간 전"||diff<86400&&Math.floor(diff/3600)+"시간 전")||day_diff===1&&"어제"||day_diff<7&&day_diff+"일 전"||day_diff<31&&Math.floor(day_diff/7)+"주 전"||day_diff<360&&Math.floor(day_diff/30)+"개월 전"||day_diff>=360&&(Math.floor(day_diff/360)===0?1:Math.floor(day_diff/360))+"년 전"}
function nl2br(str){var div=document.createElement('div');var text=document.createTextNode(str);div.appendChild(text);str=div.innerHTML;return str.replace(/\n/g,"<br />")}
function nl2brHtml(str){return str.replace(/\n/g,"<br />")}
function parseQueryString(){var str=window.location.search;var objURL={};str.replace(new RegExp("([^?=&]+)(=([^&]*))?","g"),function($0,$1,$2,$3){objURL[$1]=$3});return objURL}
function stripGALinkerParm(){var _qs=window.location.search,_path=window.location.pathname;_qs=_qs.replace(/_ga=[^&]*&?/,"");if(_qs!="?")_path+=_qs;history.replaceState({},"",_path+window.location.hash)};