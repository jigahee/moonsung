var $reportModal=$("#report-modal");if($reportModal.length>0){var $btnReportPost=$("#btn-report-post");$reportModal.on("show.bs.modal",function(e){var $btn=$(e.relatedTarget);var postId=$btn.attr("data-post-id");$reportModal.find("[name=post_id]").val(postId)});$btnReportPost.on('click',function(){$btnReportPost.loading(!0);$.ajax({url:postypeUrl+"/connector/getSessionKey",dataType:"jsonp",type:"GET",success:function(json){if(json.status==="success"){var key=json.data.key;$reportModal.find("input[name=session_key]").val(key);$reportModal.ajaxSubmit({type:"POST",dataType:'json',success:function(json){if(json.status==='success'){alertPostype(json.message,function(){$reportModal.modal("hide");$reportModal.find("textarea").val("")})}else if(json.status==='no-permission'){confirmPostype(json.message,function(){location.href=$("body").attr("data-postype-url")+"/login"},function(){})}else if(json.status==='no-activation'){confirmPostype(json.message,function(){location.href=json.failure_return_url},function(){})}else{alertPostype(json.message)}},error:function(request,status,error){},complete:function(){$btnReportPost.loading(!1)}})}}})})}
var $messageReportModal=$("#message-report-modal");if($messageReportModal.length>0){const reportMessageButton=$("#btn-report-messages");reportMessageButton.on('click',()=>{reportMessageButton.loading(!0);fetchSessionKey().then(reportMessage).then(response=>response.json()).then(report=>{const reportStatus=report.status;const reportMessage=report.message;if(reportStatus==='success'){return blockMessage().then(block=>{const alertMessage=[reportMessage,block.message].join('<br> 차단 결과 : ');alertPostype(alertMessage)})}else if(reportStatus==='no-permission'){confirmPostype(reportMessage,()=>{location.href=$("body").attr("data-postype-url")+"/login"})}else if(reportStatus==='no-activation'){confirmPostype(reportMessage,()=>{location.href=report.failure_return_url})}else if(reportStatus==='failure'){alertPostype(reportMessage)}}).catch(handleError).finally(()=>reportMessageButton.loading(!1));function fetchSessionKey(){return new Promise(((resolve,reject)=>{$.ajax({url:`${postypeUrl}/connector/getSessionKey`,dataType:'jsonp',type:'GET',}).then(result=>{if(result.status==='success'){resolve(result.data.key)}else{reject(result)}},error=>reject(error))}))}
function blockMessage(){const $conversation=$('#conversation');const userProfileId=$conversation.attr("data-user-profile-id");return new Promise(((resolve,reject)=>{$.ajax({url:`${postypeUrl}/api/message_block`,type:'GET',dataType:'jsonp',data:`user_profile_id=${userProfileId}`,}).then(result=>{resolve(result)},error=>reject(error))}))}
function reportMessage(sessionKey){const form=document.getElementById('message-report-modal');const formData=new FormData(form);formData.set('session_key',sessionKey);return fetch('/api/report',{method:'POST',headers:{'X-Requested-With':'XMLHttpRequest',},body:formData})}
function handleError(){alertPostype('메시지 신고 중 오류가 발생하였습니다. 잠시 후 다시 시도해주세요.')}})}
var $adminModal=$("#admin-modal");if($adminModal.length>0){$(".btn-gavel-post").on("click",function(){var $btn=$(this);var postId=$btn.attr("data-post-id");$adminModal.modal("show");$adminModal.find("[name=post_id]").val(postId);return!1});var $btnGavel=$("#btn-gavel");$adminModal.on("submit",function(){$btnGavel.loading(!0);var postId=$adminModal.find("[name=post_id]").val();var gavel=$adminModal.find("#gavel-number").val();var reason=$adminModal.find("#gavel-reason").val();var memo=$adminModal.find("#gavel-memo").val();$.ajax({url:postypeUrl+"/api/report_result",type:'get',dataType:'jsonp',data:"post_id="+postId+"&gavel="+gavel+"&reason="+reason+"&memo="+encodeURIComponent(memo),success:function(json){if(json.status==='success'){alertPostype(json.message,function(){window.history.go(0)})}else{alertPostype(json.message)}},complete:function(){$btnGavel.loading(!1)}});return!1})}
var commentDeleteAvailable=!0;$(document).on("click",".btn-comment-delete",function(event){event.preventDefault();var $btn=$(this);var comment_id=$btn.attr("data-comment-id");if(comment_id!==undefined&&comment_id){confirmPostype("삭제하시겠습니까?",function(){commentDeleteAvailable=!1;closeConfirmPostype();setTimeout(function(){$.ajax({url:postypeUrl+'/api/comment/'+comment_id,type:'get',dataType:'jsonp',success:function(json){if(json.status==='success'){if(json.data.user_id===0){alertPostype(json.message,function(){var $comment=$("#comment-"+comment_id);if($comment.length>0){$comment.remove()}else{window.history.go(0)}})}else{confirmPostype(json.message,function(){var userProfileId=json.data.user_profile_id;var blogId=json.data.blog_id;$.ajax({url:postypeUrl+'/api/blog/'+blogId+'/block/'+userProfileId,type:'get',dataType:'jsonp',success:function(json){if(json.status==='success'){alertPostype(json.message,function(){$("#comment-"+comment_id).remove()})}else{alertPostype(json.message,function(){$("#comment-"+comment_id).remove()})}}})},function(){$("#comment-"+comment_id).remove()})}}else{alertPostype(json.message)}},complete:function(){commentDeleteAvailable=!0}})},500)})}
return!1});var clipboard=new ClipboardJS(".btn-clipboard");$(".btn-clipboard").on("click",function(e){e.preventDefault()});clipboard.on('success',function(e){toast("URL을 복사했습니다.");e.clearSelection()});var $notifyModal=$("#notify-modal");if($notifyModal.length>0){$notifyModal.modal("show");$notifyModal.on("submit",function(){var $allowAdMessage=$notifyModal.find("#allow-ad-message");var allowAdMessage=0;if($allowAdMessage.is(":checked")){allowAdMessage=1}
$.ajax({url:postypeUrl+'/api/user/notify',type:'get',dataType:'jsonp',data:'allow_ad_message='+allowAdMessage,success:function(json){if(json.status==='success'){alertPostype(json.message)}else{alertPostype(json.message)}
$notifyModal.modal("hide")},complete:function(){}});return!1})}
